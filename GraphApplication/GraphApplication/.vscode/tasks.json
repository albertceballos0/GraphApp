{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "/usr/local/bin/g++",
            "args": [
                "-std=c++17",
                "-I/usr/local/include",
                "-I/usr/local/opt/libomp/include",
                "-fopenmp",
                "-w",
                "-Ofast",
                "-Wall",
                "main.cpp",
                "Backtracking.cpp",
                "Dijkstra.cpp",
                "BranchAndBound2.cpp",
                "Graph.cpp",
                "Greedy.cpp",
                "-o",
                "${workspaceFolder}/main"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }

        }
    ]
}